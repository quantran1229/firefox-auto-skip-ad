{

    "manifest_version": 2,
    "name": "autoSkipYTAd",
    "version": "1.0.0",

    "description": "Auto skip youtube ad if not unskippable ads",

    "icons": {
        "48": "icons/icon-48.png",
        "96": "icons/icon-48.png"
    },

    "content_scripts": [{
        "matches": ["*://*.youtube.com/*"],
        "js": ["skipAd.js"]
    }]

}